{
	"expo": {
		"name": "mendescoletor",
		"slug": "mendescoletor",
		"privacy": "public",
		"sdkVersion": "33.0.0",
		"platforms": ["ios", "android"],
		"version": "1.2.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "br.com.mendesweb.colector"
		},
		"android": {
			"package": "br.com.mendesweb.coletor",
			"googleServicesFile": "./google-services.json",
			"permissions": [
				"CAMERA",
				"ACCESS_COARSE_LOCATION",
				"ACCESS_FINE_LOCATION"
			]
		}
	}
}